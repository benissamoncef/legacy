					else
						ITMA_TTU_Commande_Desenfumage(vm_NumTube, vm_Sce_Ventilation,
										vl_sens_tunnel, vm_NumEvt, vm_CleEvt)
					*/
					/*A affichage IHM commande ventilation */                            
					NEW_TASK@("ITMA_TTU_ventilation", tl_tunnel, vm_F_tunnel)
																					   
