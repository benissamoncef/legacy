MACRO ITMA_TTU_signalisation(va_appelant, va_1, va_2, va_3, va_4)

/*
* ARGUMENTS EN ENTREE :
*  va_appelant		: Nom de la macro Applix ayant invoque la presente.
*  va_1 			: nom du tube
*  va_2             : code du tube
*  va_3             : structure vm_tunnel si va_appelant = "ITMA_TPA"
*                     vl_ficheAction si va_appelant = "ITMA_TMC"
*  va_4				: gestion des vues signalisation (1=vue 1 / 2=vue 2 / 3=vue 3)
*
* ARGUMENTS EN SORTIE : Aucun
*
*
* CODE RETOUR         : Status OK ou NOK suivant résultat
*
* CONDITION D'UTILISATION
*   Selection de Piloter -> Tunnel dans MTMT
*
* FONCTION
*    Pilote les panneaux des tunnels
*
---------------------------------------------------------------------------- */
/*A Déclaration des variables statiques */
var vl_fenetre_active	' flag de fenetre active
var vl_controle_sortie	' flag de sortie
var i,j,k			' index de loop
var vl_taille,vl_taille2 ' taille de tableau
var vl_defaut			' flag de defaut alarmes
var vl_rang			' rang selectionné dans une liste
var vl_numero			' numero selectionné
var vl_message,vl_mess,vl_messageDecoupe	' message affiché
var vl_text			' text libre
var vl_fermeture		' flag de fermeture de la fenetre par socket
var vl_F_tpm			' tableau tampon
var vl_ligne			' tableau d'une ligne
var tl_tunnel			' echangeur selectionné dans la liste
var vl_sequence		' sequence selectionné dans la liste
var vl_panneau		' tableau des panneaux à commander
var vl_etat			' tableau des etat des panneaux à commandes
var vl_type_panneau		' tableau des types de panneaux à commander
var vl_titre  		' titre du bouton
var vl_defaut_sequence	' flag de defaut sequence
var vl_indice       	' Indice de parcours des parametres 
var vl_retour		 	' code retourne à l'appelant
var vl_trouve
var vl_index, vl_index2
var vl_titre_fen
var vl_sens, vl_sens_tunnel
var vl_signalisation
var vl_meme_sens
var tl_param
var vl_trigramme
var vl_num_eqp
var vl_tab
var vl_cmd
var vl_tube_amont, vl_tube_aval
var vl_renove
var vl_icon
var x
var vl_info_task
var vl_libelle_tube
var vl_scenario_en_cours
var vl_presence_sce_desenfumage
var vl_nb_poste_BT
var vl_fichier
var vl_color

/* Controler si la tache est deja lancee */
DELAY@(0.5)
vl_info_task = TASK_LIST@(FALSE)
x = 0
FOR i = 0 TO ARRAY_SIZE@(vl_info_task) -1
	IF	UPPERCASE@(vl_info_task[i][1]) = "ITMA_TTU_SIGNALISATION" or
		UPPERCASE@(vl_info_task[i][1]) = "ITMA_TTU_ECLAIRAGE" or
		UPPERCASE@(vl_info_task[i][1]) = "ITMA_TTU_VENTILATION" or
		UPPERCASE@(vl_info_task[i][1]) = "ITMA_TTU_LT" or
		UPPERCASE@(vl_info_task[i][1]) = "ITMA_TTU_BT" or
		UPPERCASE@(vl_info_task[i][1]) = "ITMA_TTU_EXPLOITATION" or
		UPPERCASE@(vl_info_task[i][1]) = "ITMA_TTU_PILOTAGE_TUNNEL" 
	{
		x = x+1
	}
NEXT i
/* Si la tache est deja lancee on sort */
IF x > 2 EXIT@()


MACRO_WINS_BUSY@()

vm_NomSite = SYSTEM_VAR@("vg_nom_site")
vm_District = SYSTEM_VAR@("vg_site")


/*A MAJ des variables statiques */
vl_fermeture = FALSE
vm_exe_seq_en_cours = False
/*A Traitements en cas d'erreur durant l'initialisation :
*   informer l'operateur, tracer l'erreur et abandonner
* ------------------------------------------------------*/
ON ERROR 
{
	ERROR_BOX@
	COM01_Trace (COM_WARNING, COM03_Message_Erreur (C_MODULE))
	RETURN(COM_NOK)
}


vm_Appel = SUBSTRING@(va_appelant,1,8)
vm_nom_tunnel = va_1
vl_trigramme = va_2

COM01_Trace (COM_WARNING, "vm_Appel = " ++ vm_Appel)
COM01_Trace (COM_WARNING, "vm_nom_tunnel = " ++ vm_nom_tunnel)
COM01_Trace (COM_WARNING, "vl_trigramme = " ++ vl_trigramme)

SET_SYSTEM_VAR@("vg_domaine_courant", "signalisation")


'-- Appel de la fenetre depuis une FMC : récupération de la séquence à afficher
if ( vm_Appel = "ITMA_TMC" )
{
	vl_FicheAction = va_3
	vl_retour = ITMA_TTU_Lecture_PA_tunnel(vl_FicheAction.Numero_Action, vl_FicheAction.cle_action, vl_FicheAction.domaine_action)

	vm_NumTube = vl_retour[0]+0
	vm_NumSequence = vl_Retour[2]+0
	
	COM01_Trace(0, "Appel par ITMA_TMC : sequence = " ++ vm_NumSequence)
	DB_HEIGHT@(vm_fenetre_TTU,300)
	DB_XPOS@(vm_fenetre_TTU,50)
	DB_YPOS@(vm_fenetre_TTU,200)
	
}

'-- Appel de la fenetre dans le cadre d'un plan d'action : récupération de
'-- la structure vm_tunnel
if ( vm_Appel = "ITMA_TPA" )
    vm_tunnel = va_3


vl_fermeture = FALSE
vm_exe_seq_en_cours = False
vm_mode_formation = (SYSTEM_VAR@(vg_formation) <> NULL)
vm_Mode_PA 	= COM_QUITTE

vl_signalisation = TRUE
while (vl_signalisation)

/*A Tracer l'initialisation de la procedure :*/
vm_text = "------------------------------------------------------------"
COM01_Trace(0,vm_text)
vm_text = COM09_Date_Courante() ++ " Start ITMA_TTU_signalisation "

vm_text = "va_appelant : " ++ va_appelant
COM01_Trace(0,vm_text)

if NOT(IS_ARRAY@(vm_nom_tunnel))
{
    vm_text = "vm_nom_tunnel : " ++ vm_nom_tunnel
    COM01_Trace(0,vm_text)
}

'vue generique A8-A500 ou vue Toulon 
vl_fichier =  vl_trigramme ++ "/ConfigEqtSignalisation_" ++ vl_trigramme ++ ".cfg" 

vm_num_vue = va_4
if va_4 <> 1 and va_4 <> 2 and va_4 <> 3
{
	vm_num_vue = 1
}

if STRING_INDEX@(UPPERCASE@(vm_nom_tunnel),"TOULON") <> 0
{ '' specifique TOULON
	if vm_num_vue = 3
	{
		'vue Toulon tremie sud
		vl_fichier =  vl_trigramme ++ "/ConfigEqtSignalisation3_" ++ vl_trigramme ++ ".cfg"
	}
	if vm_num_vue = 2
	{
		'vue Toulon 
		vl_fichier =  vl_trigramme ++ "/ConfigEqtSignalisation2_" ++ vl_trigramme ++ ".cfg"
	}
}

/*A Chargement du fichier de config equipement signalisation du tunnel */
vm_Eqp_Signalisation = null
vl_F_tpm = READ_ASCII_FILE@(REP_RENOVE ++ vl_fichier)
vl_taille = ARRAY_SIZE@(vl_F_tpm) -1
vl_index = 0
for i = 0 to vl_taille
  '' recherche sur la ligne 1 si "pas de repere" pour ne pas afficher les reperes eqp (type eqp - eqp)
  if i=0 
  {
	if STRING_INDEX@(vl_F_tpm[i],  "pas de repere") <> 0
		tm_affiche_repere = 0
	else
		tm_affiche_repere = 1
  }
  vl_ligne = ARRAY_FROM_STRING@(vl_F_tpm[i],",")
  vl_tab = ARRAY_SIZE@(vl_ligne)
  if substring@(vl_F_tpm[i],1,1) <> "#" and vl_tab > 1
  {
    vl_taille2 = ARRAY_SIZE@(vl_ligne) -1
    for j = 0 to vl_taille2
  
      if STRING_INDEX@(vl_F_tpm[i],  "->") <> 0
      {
          vm_Eqp_Signalisation[0,j]=TRIM@(TABS_TO_SPACES@(vl_ligne[j]))
      }
      else vm_Eqp_Signalisation[vl_index,j]=TRIM@(TABS_TO_SPACES@(vl_ligne[j]))
    next j
    vl_index = vl_index + 1
  }
next i

/*A Chargement du fichier de config sequence signalisation du tunnel */
vl_F_tpm = READ_ASCII_FILE@(REP_RENOVE ++	vl_trigramme 
			++ "/ConfigSceSignalisation_" ++ vl_trigramme ++ ".cfg")
vl_taille = ARRAY_SIZE@(vl_F_tpm) -1
vl_index = 0
for i = 0 to vl_taille
  vl_ligne = ARRAY_FROM_STRING@(vl_F_tpm[i],",")
  vl_tab = ARRAY_SIZE@(vl_ligne)
  if substring@(vl_F_tpm[i],1,1) <> "#"
  {
    vl_taille2 = ARRAY_SIZE@(vl_ligne) -1
  	for j = 0 to vl_taille2
  		vm_Sce_Signalisation[vl_index,j]=TRIM@(TABS_TO_SPACES@(vl_ligne[j]))
  	next j
  	vl_index = vl_index + 1
  }
next i

SET_SYSTEM_VAR@("Liste_Sequence",vm_Sce_Signalisation)

/*A Chargement du fichier de config sequence eclairage du tunnel */
vl_F_tpm = READ_ASCII_FILE@(REP_RENOVE ++ vl_trigramme ++ "/ConfigSceEclairage_" ++ vl_trigramme ++ ".cfg")
vl_taille = ARRAY_SIZE@(vl_F_tpm) -1
vl_index = 0
for i = 0 to vl_taille
  vl_ligne = ARRAY_FROM_STRING@(vl_F_tpm[i],",")
  vl_tab = ARRAY_SIZE@(vl_ligne)
  if substring@(vl_F_tpm[i],1,1) <> "#"
  {

    vl_taille2 = ARRAY_SIZE@(vl_ligne) -1
    for j = 0 to vl_taille2
        vm_Sce_Eclairage[vl_index,j]=TRIM@(TABS_TO_SPACES@(vl_ligne[j]))
    next j
    vl_index = vl_index + 1
  }
next i

/*A Chargement du fichier de config sequence ventilation du tunnel */
vl_F_tpm = READ_ASCII_FILE@(REP_RENOVE ++ vl_trigramme ++ "/ConfigSceVentilation_" ++ vl_trigramme ++ ".cfg")
vl_taille = ARRAY_SIZE@(vl_F_tpm) -1
vl_index = 0
for i = 0 to vl_taille
  vl_ligne = ARRAY_FROM_STRING@(vl_F_tpm[i],",")
  vl_tab = ARRAY_SIZE@(vl_ligne)
  if substring@(vl_F_tpm[i],1,1) <> "#" and vl_tab > 1
  {

    vl_taille2 = ARRAY_SIZE@(vl_ligne) -1
    for j = 0 to vl_taille2

      if STRING_INDEX@(vl_F_tpm[i],  "->") <> 0
      {
        vm_Sce_Ventilation[0,j]=TRIM@(TABS_TO_SPACES@(vl_ligne[j]))
      }
      else
        vm_Sce_Ventilation[vl_index,j]=TRIM@(TABS_TO_SPACES@(vl_ligne[j]))
    next j
    vl_index = vl_index + 1
  }
next i

'' Chargement du fichier de config tunnels
vm_F_tunnel = SYSTEM_VAR@ (vg_les_donnees_config_bd_tube)

' recuperation du numero du tube
vl_rang = STRING_INDEX@(vm_Eqp_Signalisation[0][0],">")
if vl_rang >= 0
    vm_NumTube = TRIM@(SUBSTRING@(vm_Eqp_Signalisation[0][0],vl_rang+1))
else
{
    ERROR_BOX@(124,"Problème lecture paramétrage")
    EXIT@()
}

tl_tunnel[0] = vm_NumTube
tl_tunnel[1] = vm_Eqp_Signalisation[0][2]
tl_tunnel[2] = vm_Eqp_Signalisation[0][3]

/*B envoi du msg ouverture tunnel a la tache IHM principale */
vl_libelle_tube = vm_Eqp_Signalisation[0][3]
vl_cmd = vl_libelle_tube
DB_SEND_POKE@(MTTU_MSG_OUV_FENETRE,vl_cmd)



/*A Chargement de la fenetre & assignation des sockets 
*   -------------------------------------------------- */
vm_Nom_Fenetre = vm_Eqp_Signalisation[0][4]

vm_fenetre_TTU = DB_LOAD@(vm_Nom_Fenetre)

vm_messages_acceptes[0] = COM_CANAL_FIN

if ( vm_Appel <> "ITMA_TMC" )
{
	vm_messages_acceptes[1] = COM_CANAL_MTTU_ALM
	vm_messages_acceptes[2] = COM_CANAL_MTTU_PAN
	vm_messages_acceptes[3] = COM_CANAL_MTTU_SEQ
	vm_messages_acceptes[4] = COM_CANAL_MTTU_DOM_VEN
	vm_messages_acceptes[5] = COM_CANAL_MTTU_DOM_ECL
	vm_messages_acceptes[6] = COM_CANAL_MTTU_DOM_SIGT
	vm_messages_acceptes[7] = COM_CANAL_MTTU_SEQ_ECL
}
DB_ACCEPT_POKES@ (vm_fenetre_TTU, vm_messages_acceptes)

/* intégration tunnel Toulon ABE 25-09-2019 */
if STRING_INDEX@(UPPERCASE@(tl_tunnel[2]),"TOULON") <> 0
{
	if STRING_INDEX@(vm_nom_tunnel,"S1") <> 0
	{
		ITMA_TTU_ajout_fdp_sig("S1", vm_num_vue)
	}
	else
	{
		ITMA_TTU_ajout_fdp_sig("S2", vm_num_vue)
	}
}

/*A Positionnement de la fenetre & assignation de la fiche d'aide  
*   ------------------------------------------------------------- */
DB_XPOS@(vm_fenetre_TTU, 0)
DB_YPOS@(vm_fenetre_TTU, 115)
DB_WIDTH@(vm_fenetre_TTU, 993)
DB_HEIGHT@(vm_fenetre_TTU, 715)

DEFINE_HELPFILE@("gerer_pilotage_tunnel", "./aid/ITMA_TTU_tunnel.aide")
DB_HELP_TOPIC@(vm_fenetre_TTU,"gerer_pilotage_tunnel")


/* charger le bandeau de navigation */
ITMA_TTU_bandeau(vm_fenetre_TTU, vl_trigramme, "", vm_nom_tunnel)

 
/*A affichage le titre de la boite 
*   -------------------------------- */
DB_TITLE@(vm_fenetre_TTU,"Signalisation Tunnel  " ++ vl_libelle_tube)
vm_Titre_Fenetre = "Signalisation Tunnel  " ++ vl_libelle_tube

DB_CTRL_TITLE@(vm_fenetre_TTU,"Li_nom","Signalisation Tunnel  " ++ vl_libelle_tube)

'' creation des equipements commande et etat
ITMA_TTU_creation_eqp_signalisation

DB_CTRL_DISPLAY@(vm_fenetre_TTU,"BP_defaut",False)


/*A Boucle de capture des événements de la boite de dialogue  
*   -------------------------------------------------------- */
vl_fenetre_active = TRUE

/*A appel de la liste des sequences */
DB_DISPLAY_ONLY@(vm_fenetre_TTU,True)
DB_DISPLAY@(vm_fenetre_TTU)
DB_DISPLAY_ONLY@(vm_fenetre_TTU,False)

DB_CTRL_DISPLAY@(vm_fenetre_TTU,"BP_lier",True)
DB_CTRL_DISPLAY@(vm_fenetre_TTU,"LI_Evenements",False)
DB_CTRL_DISPLAY@(vm_fenetre_TTU,"TA_Evenements",False)
DB_CTRL_GRAYED@(vm_fenetre_TTU,"BP_executer",True)
DB_CTRL_GRAYED@(vm_fenetre_TTU,"BP_refuser",True)
DB_CTRL_GRAYED@(vm_fenetre_TTU,"BP_ok",True)
DB_CTRL_GRAYED@(vm_fenetre_TTU,"BP_nul",True)

if STRING_INDEX@(UPPERCASE@(vm_nom_tunnel),"TOULON") <> 0
{ '' specifique TOULON
	'' griser le bouton 'BP_signalisation' ou  'BP_signalisation2' ou  'BP_signalisation3'
	if vm_num_vue = 3
	{
		'vue transversale exploitation autres
		DB_CTRL_GRAYED@(vm_fenetre_TTU,"BP_signalisation3",True)
	}
	if vm_num_vue = 2
	{
		'vue transversale exploitation coté Est
		DB_CTRL_GRAYED@(vm_fenetre_TTU,"BP_signalisation2",True)
	}
	if vm_num_vue = 1
	{
		'vue longitudinale accélérateurs et injecteurs (exploitation coté Est pout Toulon)
		DB_CTRL_GRAYED@(vm_fenetre_TTU,"BP_signalisation",True)
	}
}
else
{
	DB_CTRL_GRAYED@(vm_fenetre_TTU,"BP_signalisation",True)
}

if ( vm_Appel<>"ITMA_TMC" ) vm_NumSequence = XDC_SIG_SEQ_HORS_SEQUENCE		' Sequence Hors Sequence


'-- Appel de la fenetre dans le cadre d'un plan d'action
'-- Choix de la séquence en automatique
if ( vm_Appel="ITMA_TPA" )
{
    vm_NumEvt = vm_tunnel.NumEvt
    vm_CleEvt = vm_tunnel.CleEvt
    vm_NumSequence = vm_tunnel.Sequence
	
	if vm_NumSequence = XDC_TUB_ETEINT { vm_NumSequence = XDC_SIG_SEQ_RETOUR_NORMAL } ' gestion sequence 0 depuis le SAD
    vl_sequence = ITMA_TTU_sequence_PA(vl_trigramme, vm_NumSequence)
    
    if vl_sequence[0] <> -1
    {
        vm_exe_seq_en_cours = False
        vm_seq_select = SYSTEM_VAR@("sequence")

        vl_text = "Scénario envisagé : " ++ vm_seq_select
        DB_CTRL_TITLE@(vm_fenetre_TTU,"Li_status",vl_text)

        DB_CTRL_GRAYED@(vm_fenetre_TTU,"BP_executer",False)
        DB_CTRL_GRAYED@(vm_fenetre_TTU,"BP_refuser",False)
        DB_CTRL_GRAYED@(vm_fenetre_TTU,"BP_lier",True)
