/*X*/
/* ----------------------------------------------------------------------------
* SERVICE RENDU :
*  Affiche la vue du district de nice
* 
-------------------------------------------------------------------------------
* SEQUENCE D'APPEL :
*/

MACRO	ITMA_TVU_D_NICE()

/*
* ARGUMENTS EN ENTREE : Aucun
*	
*				
* ARGUMENTS EN SORTIE : Aucun
*
*
* CODE RETOUR         : XDC_OK ou XDC_NOK
*
* CONDITION D'UTILISATION
*
*	Appele depuis le menu principal MTMT  
*
* FONCTION
*
---------------------------------------------------------------------------- */

/*A
 * Traitements en cas d'erreur :
 * tracer l'erreur et abandonner
 * -----------------------------------------------------
 */
VAR	vl_rpc_retour

ON ERROR {
    ERROR_BOX@
    COM01_Trace (COM_WARNING, COM03_Message_Erreur (C_MODULE))
    RETURN
}
/*A Initialiser la connection RPC*/

	ITMA_TPC_init_connection("", CM_Socket_RPC)

/*A demander l'affichage */

	ITMA_TPC_appel_fct (CM_AFFICH_VUE, "Nice", vl_rpc_retour)

/*A Fermer la connection RPC */

	ITMA_TPC_fin_connection ()

ENDMACRO

