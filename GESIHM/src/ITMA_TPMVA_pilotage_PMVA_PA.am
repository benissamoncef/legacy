  		vm_PMV[va_type].Message2 		= vl_PMV.Message2
	 	vm_PMV[va_type].Message3 		= vl_PMV.Message3
	 	vm_PMV[va_type].Message4 		= vl_PMV.Message4
	 	vm_PMV[va_type].Message5 		= vl_PMV.Message5
