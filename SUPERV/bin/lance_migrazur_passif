#!/bin/ksh
#
#       -- Script de lancement de l'application MIGRAZUR
#               sur un SD ou SC  PASSIF
#

# Modification du PATH
export PATH=$PATH:/produits/migrazur/appliSD/exec

# Initialisation des variables d'environnement specifiques
# a l'application
export exec_migrazur=/produits/migrazur/appliSD/exec


#       Premierement , lancement de l'executable de mise a jour
#       de l'heure system, lue par la centrale horaire
init_heure.x

#       Deuxiemement lancement de l'applicatif MIGRAZUR
rm /nohup.out
/bin/nohup tsaplg.x tsetat.x &

