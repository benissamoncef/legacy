declare @nom_base varchar(10)

/* nom de la base courante */
select @nom_base = db_name()

if @nom_base = 'CFG' 
	begin   print " "
	execute sp_foreignkey ANN_CMP, ANN_AST_CMP, competence
	execute sp_foreignkey ANN_CMP, ANN_FIC, interlocuteur
	end

if @nom_base = 'CFT'
	begin   print " "
	end

if @nom_base = 'EXP'
	begin   print " "
	execute sp_foreignkey ANN_CMP, ANN_AST_CMP, competence
	execute sp_foreignkey ANN_CMP, ANN_FIC, interlocuteur
	end

go
