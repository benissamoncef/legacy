/* ============================================================ */
/*   Nom de la base   :  MIGRAZUR                               */
/*   Nom de SGBD      :  SYBASE10                               */
/*   Date de creation :  26/06/2023                              */
/* ============================================================ */

/* ============================================================ */
/*   Table : RES_PRA                                            */
/* ============================================================ */

/* Suppression de la table si elle existe */
/* ===========================*/
if exists (select * from sysobjects where name = 'RES_PRA' and type = 'U')
	begin
	drop table RES_PRA
	end
go

declare @nom_base varchar(10)

/* nom de la base courante */
select @nom_base = db_name()

/*
*  Fonctionalit� de la table
*  -------------------------
*  Contient les districts
*
*  Description des colonnes de la table
*  ------------------------------------
*        numero    : numero d'un district
*        nom       : nom de ce district
*        code      : code de ce district
*        type      : type de ce district
*        serveur   : nom serveur de donn�es
*        machine   : nom machine du serveur de donn�es
*/


create table RES_PRA
(
    numero                          tinyint               not null,
    code                            char(2)               not null,
    serveur_pra                     char(6)               null
)
/* ajout des droits a public*/
grant all on RES_PRA to public

/* =============================================================== */
/*   creation des cles primaires et des index en fonction de la base courante                     */
/* =============================================================== */
if @nom_base = 'CFG'
	begin
	/* ========================================================== */
	/*   Cle primaire : numero                                      */
	/* ========================================================== */
	execute sp_primarykey RES_PRA, numero

	/* ========================================================== */
	/*   Index : RES_PRA_PK                                         */
	/* ========================================================== */
	create unique index RES_PRA_PK on RES_PRA (numero)
	end

if @nom_base = 'CFT'
	begin
	/* ========================================================== */
	/*   Cle primaire : numero                                      */
	/* ========================================================== */
	execute sp_primarykey RES_PRA, numero

	/* ========================================================== */
	/*   Index : RES_DIS_PK                                         */
	/* ========================================================== */
	create unique index RES_PRA_PK on RES_PRA (numero)
	end

go
